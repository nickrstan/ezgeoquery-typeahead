{
  "name": "ezgeo-typeahead",
  "version": "0.2.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run serve:dev",
    "serve": "npm run serve:dev",
    "serve:dev": "webpack-dev-server --mode development --config ./webpack.config.js --inline --progress --watch",
    "serve:prod": "cross-env NODE_ENV=production webpack-dev-server  --mode production --config ./webpack.config.js --port 3000 --host 0.0.0.0 --inline --progress  --watch --content-base dist/",
    "build": "npm run build:dev",
    "build:dev": "webpack --mode development --config ./webpack.config.js --progress --color --display-error-details",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.js  --progress  --color --display-error-details --bail"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.3.6",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.0.1",
    "css-loader": "^1.0.0",
    "ejs-loader": "^0.3.0",
    "eslint": "^5.4.0",
    "html-loader": "^0.5.5",
    "jquery": "^3.3.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.22.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.0.1",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/node": "^10.9.4"
  }
}
